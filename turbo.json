{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"]
        },
        "clean": {
            "dependsOn": ["^clean"]
        },
        "release": {
            "dependsOn": ["^release"]
        },
        "test": {
            "dependsOn": ["^test"]
        },
        "link": {
            "dependsOn": ["^link"]
        },
        "unlink": {
            "dependsOn": ["^unlink"]
        },
        "type-check": {
            "dependsOn": ["^type-check"]
        }
    }
}
