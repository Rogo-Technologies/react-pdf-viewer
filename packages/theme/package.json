{
    "name": "@rogo-technologies/react-pdf-viewer-theme",
    "version": "3.12.1",
    "description": "A React component to view a PDF document",
    "type": "module",
    "keywords": [
        "react",
        "react.js",
        "pdf",
        "pdf.js",
        "pdf viewer"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Rogo-Technologies/react-pdf-viewer",
        "directory": "packages/theme"
    },
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/cjs/*.js",
        "lib/index.d.ts",
        "lib/index.js"
    ],
    "dependencies": {
        "@rogo-technologies/react-pdf-viewer-core": "3.12.1"
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "clean": "rm -rf lib && mkdir lib",
        "build": "npm run clean && cp src/index.d.ts lib && cp dist/index.js lib && rollup -c ../../rollup.config.js",
        "test": "jest --config ../../jest.config.js",
        "release": "npm publish --access public"
    }
}
